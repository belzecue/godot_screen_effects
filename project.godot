; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ScreenEffects"
run/main_scene="res://example_project/example_map/example_map.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

EffectsHandler="*res://addons/screen_effects/scripts/effects_handler.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[editor]

movie_writer/movie_file="E:/_gamedev/godot_screen_effects/movie.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/screen_effects/plugin.cfg")
